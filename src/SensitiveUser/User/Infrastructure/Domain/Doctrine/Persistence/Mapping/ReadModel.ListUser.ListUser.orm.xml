<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="SensitiveUser\User\Domain\ReadModel\ListUser\ListUser" table="`list_users`">
        <id name="userId" type="UserId" column="user_id"/>
        <field name="email" type="Email" column="email" nullable="false"/>
        <field name="address" type="string" column="address" nullable="true"/>
        <field name="age" type="integer" column="age" nullable="true"/>
        <field name="height" type="float" column="height" nullable="true"/>
        <field name="characteristics" type="simple_array" column="characteristics" nullable="true"/>
    </entity>
</doctrine-mapping>
